<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.unburden.ui.TaskListPanel">
    <stylesheets>
        <URL value="@DarkTheme.css" />
        <URL value="@Extensions.css" />
    </stylesheets>
    <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="No." />
          <Menu mnemonicParsing="false" text="                Task                      " />
          <Menu mnemonicParsing="false" text="          Date      " />
            <Menu mnemonicParsing="false" text="       Start   " />
            <Menu mnemonicParsing="false" text="           End       " />
            <Menu mnemonicParsing="false" text="            Tags         " />
            <Menu mnemonicParsing="false" text="    Status  " />
        </menus>
      </MenuBar>
        <ListView fx:id="taskListView" prefWidth="248.0" VBox.vgrow="ALWAYS" />
    </children>
</VBox>

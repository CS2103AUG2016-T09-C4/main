<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<!--@@Gauri Joshi A0143095H  -->

<AnchorPane fx:id="summary" maxWidth="350.0" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.unburden.ui.SummaryPanel">
   <children>
      <GridPane layoutX="14.0" prefHeight="300.0" prefWidth="300.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="160.0" minWidth="10.0" prefWidth="151.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="199.0" minWidth="10.0" prefWidth="149.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField maxWidth="90.0" minWidth="90.0" prefHeight="0.0" prefWidth="150.0" text="Done" GridPane.rowIndex="1" />
            <TextField maxWidth="90.0" minWidth="90.0" text="Undone" GridPane.rowIndex="2" />
            <TextField maxWidth="90.0" minWidth="90.0" prefHeight="23.0" prefWidth="95.0" text="Today" />
            <Label fx:id="today" text="#Today" GridPane.columnIndex="1" />
            <Label fx:id="done" text="#Done" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="undone" text="#Undone" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>

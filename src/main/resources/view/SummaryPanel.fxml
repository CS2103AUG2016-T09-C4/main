<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<!--@@Gauri Joshi A0143095H  -->

<AnchorPane fx:id="summary" maxWidth="350.0" prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.unburden.ui.SummaryPanel">
   <children>
      <GridPane layoutX="14.0" prefHeight="300.0" prefWidth="300.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="160.0" minWidth="10.0" prefWidth="151.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="199.0" minWidth="10.0" prefWidth="149.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <MenuBar>
               <menus>
                  <Menu mnemonicParsing="false" text="Today" />
               </menus>
            </MenuBar>
            <Label fx:id="today" text="#Today" GridPane.columnIndex="1" />
            <Label fx:id="done" text="#Done" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="undone" text="#Undone" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <MenuBar prefHeight="25.0" prefWidth="74.0">
              <menus>
                <Menu mnemonicParsing="false" text="Today" />
              </menus>
            </MenuBar>
            <MenuBar GridPane.rowIndex="1">
               <menus>
                  <Menu mnemonicParsing="false" text="Done" />
               </menus>
            </MenuBar>
            <MenuBar GridPane.rowIndex="2">
               <menus>
                  <Menu mnemonicParsing="false" text="Undone" />
               </menus>
            </MenuBar>
         </children>
      </GridPane>
   </children>
</AnchorPane>
